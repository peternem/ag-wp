{
    "name": "aga-theme",
    "title": "Agalite WP Theme ",
    "description": "WordPress starter theme based on zen",
    "version": "1.0.0",
    "license": "MIT",
    "main": "gulpfile.babel.js",
    "scripts": {
        "start": "gulp --dev --mode development",
        "dev": "gulp build --dev --mode development",
        "build": "gulp build --production --mode production",
        "package": "gulp package --production --mode production"
    },
    "browserslist": [
        "last 2 version",
        "> 1%",
        "maintained node versions",
        "not dead"
    ],
    "keywords": [
        "WordPress",
        "starter theme",
        "semantic",
        "mobile first",
        "customizable",
        "responsive"
    ],
    "dependencies": {
        "font-awesome": "^4.7.0",
        "include-media": "^1.4.9",
        "jquery": "^3.4.1",
        "motion-ui": "^2.0.3",
        "postcss-reporter": "^6.0.1",
        "postcss-scss": "^2.0.0",
        "slick-carousel": "^1.8.1",
        "stylelint": "^11.1.1",
        "stylelint-order": "^3.1.1",
        "what-input": "^5.2.6"
    },
    "devDependencies": {
        "ansi-colors": "^4.1.1",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-es2015": "^6.3.13",
        "babel-register": "^6.26.0",
        "browser-sync": "^2.27.10",
        "dateformat": "^3.0.2",
        "fancy-log": "^1.3.2",
        "gulp": "^4.0",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.0.0",
        "gulp-cli": "^2.0.1",
        "gulp-concat": "^2.5.2",
        "gulp-extname": "^0.2.0",
        "gulp-if": "^3.0.0",
        "gulp-imagemin": "^6.1.1",
        "gulp-load-plugins": "^2.0.1",
        "gulp-phpcbf": "^0.1.2",
        "gulp-phpcs": "^3.1.0",
        "gulp-postcss": "^8.0.0",
        "gulp-rev": "^9.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-sass-lint": "^1.4.0",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-uglify": "^3.0.2",
        "gulp-zip": "^5.0.1",
        "js-yaml": "^3.13.0",
        "panini": "^1.6.3",
        "rimraf": "^3.0.0",
        "style-sherpa": "^1.0.0",
        "vinyl-named": "^1.1.0",
        "webpack": "^4.41.2",
        "webpack-stream": "^5.2.1",
        "yargs": "^14.2.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/peternem/ag-wp"
    },
    "babel": {
        "presets": [
            "es2015"
        ]
    }
